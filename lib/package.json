{
  "name": "pageview-counter",
  "version": "0.1.0",
  "description": "A simple pageview counter for your website",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./next-app": {
      "types": "./dist/next-app.d.ts",
      "import": "./dist/next-app.mjs",
      "require": "./dist/next-app.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "react",
    "picker",
    "icon-picker",
    "icon",
    "ui",
    "iconify"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "react": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
